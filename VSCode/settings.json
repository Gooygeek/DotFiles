{
    "asciidecorator.defaultFont": "Big",
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.startOnNewLine": true,
    "blockman.n01LineHeight": 0,
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "on",
    "breadcrumbs.symbolPath": "on",
    "cSpell.userWords": [
        "CIDR",
        "Executables",
        "Lifecycle",
        "NACL",
        "OnPrem",
        "SG",
        "Splunk",
        "Unscanned",
        "behaviour",
        "cluster",
        "customisation",
        "customise",
        "io",
        "kubernetes",
        "poc",
        "subnet",
        "utilised",
        "amazonaws"
    ],
    "css.lint.idSelector": "warning",
    "debug.inlineValues": true,
    "debug.toolBarLocation": "docked",
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.wordWrap": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "'0xProto Nerd Font', 'FiraMono Nerd Font', 'Consolas', 'ComicMono Nerd Font', 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.guides.bracketPairs": false,
    "editor.guides.highlightActiveIndentation": false,
    "editor.guides.indentation": false,
    "editor.inlineSuggest.enabled": true,
    "editor.inlineSuggest.fontFamily": "'ComicMono Nerd Font', 'Courier New', monospace",
    "editor.minimap.maxColumn": 200,
    "editor.minimap.renderCharacters": true,
    "editor.minimap.showSlider": "always",
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.preview": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.wordWrap": "off",
    "editor.wrappingIndent": "indent",
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "**/*.hcl": "terraform"
    },
    "files.autoSave": "onWindowChange",
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.ignoreMissingGitWarning": true,
    "git.mergeEditor": true,
    "github.copilot.enable": {
        "*": true
    },
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.currentLine.format": "${message} â€¢ ${author}, ${agoOrDate}",
    "gitlens.defaultDateFormat": "YYYY-MMM-DD-kk:mm:ssZZ",
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.statusBar.alignment": "left",
    "gitlens.views.repositories.files.layout": "tree",
    "go.toolsManagement.autoUpdate": true,
    "indentRainbow.errorColor": "rgba(128,32,32,0.6)",
    "indentRainbow.ignoreErrorLanguages": [
        "plaintext"
    ],
    "markdown.styles": [
        "./github-markdown-dark.css"
    ],
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "problems.showCurrentInStatus": false,
    "python.analysis.completeFunctionParens": true,
    "python.autoComplete.addBrackets": true,
    "python.formatting.provider": "black",
    "python.formatting.yapfArgs": [
        "--style",
        "{blank_line_before_nested_class_or_def: True, column_limit: 180, dedent_closing_brackets: True, indent_dictionary_value: True, split_complex_comprehension: True}"
    ],
    "python.jediEnabled": false,
    "python.languageServer": "Pylance",
    "python.testing.pytestEnabled": true,
    "redhat.telemetry.enabled": false,
    "remote.SSH.defaultExtensions": [],
    "remote.SSH.showLoginTerminal": true,
    "svelte.enable-ts-plugin": true,
    "tabnine.experimentalAutoImports": true,
    "telemetry.telemetryLevel": "off",
    "terminal.external.windowsExec": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "terminal.integrated.cursorBlinking": true,
    "terraform.languageServer.enable": true,
    "vsicons.associations.files": [
        {
            "icon": "cloudformation",
            "extensions": [
                "cf.yaml",
                "cf.json"
            ],
            "format": "svg"
        },
        {
            "icon": "cloudformation-bad",
            "extensions": [
                "cf.yml"
            ],
            "format": "svg"
        },
        {
        "filenamesGlob": ["Taskfile", "Taskfile.dist"],
        "extensionsGlob": ["yml", "yaml"],
        "filename": true,
        "format": "svg",
        "icon": "taskfile"
        }
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "vsicons.presets.angular": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path.windows": "C:\\Users\\kgoldsworthy\\.vs-kubernetes\\tools\\minikube\\windows-amd64\\minikube.exe"
    },
    "window.commandCenter": true,
    "window.title": "${activeEditorMedium}${separator}${dirty}",
    "window.titleBarStyle": "custom",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#1073cf2d",
        "editor.lineHighlightBorder": "#9fced11f",
        "tab.unfocusedActiveBorder": "#fff0"
    },
    "workbench.colorTheme": "Monokai",
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.editor.openPositioning": "last",
    "workbench.editor.scrollToSwitchTabs": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "workbench.tree.enableStickyScroll": true,
    "yaml.customTags": [
        "!Ref",
        "!GetAtt",
        "!FindInMap sequence",
        "!FindInMap scalar",
        "!GetAtt sequence",
        "!GetAtt scalar",
        "!GetAZs",
        "!Cidr",
        "!ImportValue",
        "!Join sequence",
        "!Select sequence",
        "!Split",
        "!Sub sequence",
        "!Sub scalar",
        "!And",
        "!Equals sequence",
        "!If",
        "!Not",
        "!Or",
        "!Base64"
    ],
    "yaml.format.enable": true,
    "yaml.format.proseWrap": "always",
    "yaml.format.singleQuote": true,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[go]": {
        "editor.defaultFormatter": "golang.go"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
        "editor.tabSize": 2,
        "editor.wordWrap": "off"
    },
    "[python]": {
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode",
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust",
        "editor.formatOnSave": true
    },
    "[terraform]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "hashicorp.terraform"
    }
}
